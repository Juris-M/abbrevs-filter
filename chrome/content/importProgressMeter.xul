<!DOCTYPE window SYSTEM "chrome://abbrevs-filter/locale/import.dtd">

<window
	id="abbrevs-filter-import-dialog"
	orient="vertical"
	title="Building abbreviation database. Please wait, we start slowly but we'll speed up."
	width="700" 
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
	persist="screenX screenY">
	
	<script src="importProgressMeter.js"/>
    
	<box orient="horizontal" id="abbrevs-filter-popup" style="background-color:white;"> 
        <progressmeter id="abbrevs-filter-progress" mode="determined" value="0" flex="1"/>        
	</box>
</window>
